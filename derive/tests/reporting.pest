a = { "a" }
b = { "b" }
c = { "c" }
d = { any }

choices = _{ a | b | c }
choices_no_progress = { a | b | c }
choices_a_progress = { a ~ a | b | c }
choices_b_progress = { a | b ~ b | c }

level1 = _{ level2 }
